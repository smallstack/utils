{
  "name": "@smallstack/utils",
  "version": "0.1.4",
  "description": "Opinionated selection of util functions used by smallstack projects",
  "keywords": [
    "utils",
    "smallstack"
  ],
  "author": "smallstack GmbH",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/utils.umd.js",
  "module": "./dist/utils.es.js",
  "types": "./dist/utils.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/utils.es.js",
      "require": "./dist/utils.umd.js",
      "types": "./dist/main.d.ts"
    },
    "./string": {
      "import": "./dist/utils.string.es.js",
      "require": "./dist/utils.string.umd.js",
      "types": "./dist/modules/string/index.d.ts"
    },
    "./number": {
      "import": "./dist/utils.number.es.js",
      "require": "./dist/utils.number.umd.js",
      "types": "./dist/modules/number/index.d.ts"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest --coverage"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@vitest/coverage-v8": "^2.1.5",
    "jsdom": "^25.0.1",
    "typescript": "^5.6.3",
    "vite": "^5.4.11",
    "vite-plugin-dts": "^4.3.0",
    "vitest": "^2.1.5"
  },
  "volta": {
    "node": "22.11.0"
  }
}
